{
  "name": "planit",
  "version": "1.0.0",
  "description": "Teen-friendly planner theme using TailwindCSS",
  "scripts": {
    "build": "npx tailwindcss -i ./src/input.css -o ./src/output.css --minify",
    "watch": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.12",
    "tailwindcss": "^4.1.12",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0"
  }
}